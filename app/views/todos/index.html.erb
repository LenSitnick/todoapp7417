<h1>List of all ToDos</h1>
<p>
  <%= link_to "Create a ToDo", new_todo_path %>
</p>
<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <% @todos.each do |todo|%>
      </tr>
        <td>
          <%= todo.name %>
        </td>
        <td>
          <%= todo.description %>
        </td>
        <td>
          <%= link_to "Edit", edit_todo_path(todo) %>
        </td>
        <td>
          <%= link_to "Show", todo_path(todo) %>
        </td>
    <% end %>
  
</table>
